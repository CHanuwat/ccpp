<?xml version="1.0" encoding="UTF-8"?>
<templates>
    <t t-name="ccpp.CCPPInternalDashboardListManager" owl="1">
        <div class="o_ccpp_dashboard_list container-fluid py-4 border-bottom bg-white">
            <div class="row justify-content-between gap-3 gap-lg-0">
                <div class="col-12 col-lg-12 col-xl-12 col-xxl-12 flex-grow-1 flex-lg-grow-0 flex-shrink-0">
                    
                    <div class="gap-4" style="padding-bottom:5px">
                        <div class="grid gap-2">
                            <div class="g-col-2 p-0" t-on-click="setSearchContext" title="My Customer">
                                <a href="#" class="btn btn-primary w-100 h-100 border-0 rounded-0 text-capitalize fw-normal">
                                    <div class="fs-2" t-out="internalData['my_customer']"/>My Customer
                                </a>
                            </div><div class="g-col-2 p-0" t-on-click="setSearchContext" title="All Customer">
                                <a href="#" class="btn btn-primary w-100 h-100 border-0 rounded-0 text-capitalize fw-normal">
                                    <div class="fs-2" t-out="internalData['all_customer']"/>All Customer
                                </a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </t>
</templates>
